{
  "name": "eq-rts-map",
  "version": "1.0.0-beta.2",
  "type": "commonjs",
  "main": "electron/main.js",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development npx electron .",
    "electron:dev": "concurrently \"bun run dev\" \"bun run electron\" -k",
    "electron:start": "cross-env NODE_ENV=production npx electron .",
    "electron:build": "bun run build && npx electron-builder",
    "electron:build:mac": "bun run build && npx electron-builder --mac",
    "electron:build:win": "bun run build && npx electron-builder --win",
    "electron:build:linux": "bun run build && npx electron-builder --linux",
    "electron:build:fast": "bun run build && npx electron-builder --mac --config.compression=normal",
    "electron:dist": "bun run build && npx electron-builder --publish=never",
    "electron:dist:mac": "bun run build && npx electron-builder --mac --publish=never",
    "electron:dist:win": "bun run build && npx electron-builder --win --publish=never",
    "electron:dist:linux": "bun run build && npx electron-builder --linux --publish=never"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/bun": "latest",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "concurrently": "^9.2.1",
    "critters": "^0.0.25",
    "cross-env": "^10.1.0",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-virtual": "^3.13.12",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "i18next": "^25.6.0",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.545.0",
    "maplibre-gl": "^5.9.0",
    "next": "^15.5.5",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.0.1",
    "react-map-gl": "^8.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  }
}
